<form action = "/users/<%=@user.id%>/walks" method = "post">
  <% if @user.dogs %>
    <% @user.dogs.each do |dog| %>
      <input type = "hidden" name="dogs[<%=dog.id%>]" value='<%=dog.id%>'></input>
      <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <% end %>
  <% end %>
  <input class = "button small radius small-8 small-centered success columns " type = "submit" value = "Take A Walk!"></input>
</form>
