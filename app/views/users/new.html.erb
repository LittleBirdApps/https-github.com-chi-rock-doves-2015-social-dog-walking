<%= form_for @user do |f| %>

  <%= f.label :petname %>
  <%= f.text_field :petname %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "register" %>
<% end %>

<% @errors.each do |error| %>
  <%= error %>
<% end %>
